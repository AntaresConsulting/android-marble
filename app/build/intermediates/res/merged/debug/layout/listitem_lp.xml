<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:minHeight="40dp"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/nombreProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="false"
        android:layout_margin="30dp"
        android:layout_marginBottom="50dp"
        android:text="TextView"
        android:textSize="22sp" />

    <TextView
        android:id="@+id/titleCant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/cantPlacaUtil"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="25dp"
        android:layout_toRightOf="@id/nombreProd"
        android:text="@string/listitem_quantity_units"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/cantidadInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/titleCant"
        android:layout_below="@+id/titleCant"
        android:layout_marginLeft="20dp"
        android:text="999"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/unidades"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/cantidadInfo"
        android:layout_alignBottom="@+id/cantidadInfo"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/cantidadInfo"
        android:text="@string/lbl_unidades"
        android:textAppearance="?android:attr/textAppearanceSmall" />
<RelativeLayout
        android:id="@+id/dimFrmContainer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/titleCant"
        android:layout_centerVertical="true"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/titleCant" >

        <TextView
            android:id="@+id/titleDim"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="5dp"
            android:text="@string/listitem_dimensions"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dimH"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/titleDim"
            android:text="Small Text"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dimW"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/titleDim"
            android:layout_marginLeft="46dp"
            android:layout_toRightOf="@+id/dimH"
            android:text="Small Text"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dimSeparator"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/titleDim"
            android:layout_marginLeft="24dp"
            android:layout_toRightOf="@+id/dimH"
            android:text="X"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/dimW"
            android:layout_alignBottom="@+id/dimW"
            android:layout_marginLeft="14dp"
            android:layout_toRightOf="@+id/dimW"
            android:text="X"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dimT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/titleDim"
            android:layout_marginLeft="18dp"
            android:layout_toRightOf="@+id/TextView02"
            android:text="Small Text"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/superficieLbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/dimH"
            android:layout_marginTop="5dp"
            android:text="@string/listitem_superficie"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dimH2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/superficieLbl"
            android:layout_toRightOf="@id/superficieLbl"
            android:layout_marginLeft="10dp"
	        android:text="Small Text"
            android:textAppearance="?android:attr/textAppearanceSmall" />
        <TextView
            android:id="@+id/TextView06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/dimH2"
            android:layout_toRightOf="@id/dimH2"
            android:layout_marginLeft="10dp"
            android:text="X"
            android:textAppearance="?android:attr/textAppearanceSmall" />
        <TextView
            android:id="@+id/dimW2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/TextView06"
            android:layout_toRightOf="@id/TextView06"
            android:layout_marginLeft="10dp"
            android:text="Small Text"
            android:textAppearance="?android:attr/textAppearanceSmall" />



        <TextView
            android:id="@+id/superficie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/dimW2"
            android:layout_toRightOf="@id/dimW2"
            android:layout_marginLeft="10dp"
            android:text="= XXX m2" />
        <TextView
    android:id="@+id/TextView01"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
	android:layout_toRightOf="@+id/superficie"
	android:layout_alignBaseline="@+id/superficie"
    android:text="@string/listitem_mp_units"
    android:layout_marginLeft="10dp"
    android:textAppearance="?android:attr/textAppearanceSmall" />
        
        <TextView
    android:id="@+id/supTotalLbl"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignLeft="@+id/superficieLbl"
    android:layout_below="@+id/superficieLbl"
    android:layout_marginTop="5dp"
    android:text="@string/listitem_area_total"
    android:textAppearance="?android:attr/textAppearanceSmall" />

<TextView
    android:id="@+id/superficieTotal"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignBottom="@+id/supTotalLbl"
    android:layout_marginLeft="10dp"
    android:layout_toRightOf="@+id/supTotalLbl"
    android:text="xxx m2"
    android:textAppearance="?android:attr/textAppearanceSmall" />

<TextView
    android:id="@+id/TextView01"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
	android:layout_toRightOf="@+id/superficieTotal"
	android:layout_alignBaseline="@+id/superficieTotal"
    android:text="@string/listitem_mp_units"
    android:layout_marginLeft="10dp"
    android:textAppearance="?android:attr/textAppearanceSmall" />

    </RelativeLayout>


</RelativeLayout>